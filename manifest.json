{
	"manifest_version": 2,
	"name": "saveTabs",
	"version": "1.0",

	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"48": "/icon.svg"
		},
		"default_title": "saveTabs",
		"default_popup": "/popup.html"
	},

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [
		"storage",
		"menus",
		"activeTab"
	]
}